#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/eden/Desktop/trainers_meal/node_modules/tsconfck/bin/node_modules:/Users/eden/Desktop/trainers_meal/node_modules/tsconfck/node_modules:/Users/eden/Desktop/trainers_meal/node_modules:/Users/eden/Desktop/node_modules:/Users/eden/node_modules:/Users/node_modules:/node_modules:/Users/eden/Desktop/trainers_meal/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/eden/Desktop/trainers_meal/node_modules/tsconfck/bin/node_modules:/Users/eden/Desktop/trainers_meal/node_modules/tsconfck/node_modules:/Users/eden/Desktop/trainers_meal/node_modules:/Users/eden/Desktop/node_modules:/Users/eden/node_modules:/Users/node_modules:/node_modules:/Users/eden/Desktop/trainers_meal/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsconfck/bin/tsconfck.js" "$@"
else
  exec node  "$basedir/../tsconfck/bin/tsconfck.js" "$@"
fi
